<?xml version="1.0" encoding="UTF-8"?>

<facelet-taglib
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
    version="2.0">
    
    <namespace>customfunction.transport</namespace>

    <function>
        <function-name>containAuthorize</function-name>
        <function-class>transport.customfunction.Functions</function-class>
        <function-signature>boolean containAuthorize(java.util.Collection, java.lang.String)</function-signature>
    </function>
    
    <function>
        <function-name>containRole</function-name>
        <function-class>transport.customfunction.Functions</function-class>
        <function-signature>boolean containRole(java.util.Collection, java.lang.String)</function-signature>
    </function>
    
    <function>
    	<function-name>compareDate</function-name>
    	 <function-class>transport.customfunction.Functions</function-class>
    	 <function-signature>boolean compareDate(java.util.Date, java.util.Date)</function-signature>
    </function>
</facelet-taglib>